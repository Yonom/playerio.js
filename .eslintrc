{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "valid-jsdoc": [1, {
      "prefer": {
        "return": "returns"
      },
      "requireReturn": false,
      "requireParamDescription": false,
      "requireReturnDescription": false
    }]
  }
}
